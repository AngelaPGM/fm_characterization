<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="autolayoutconstraints" value="false"/>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="Pizza">
			<or mandatory="true" name="Topping">
				<graphics key="collapsed" value="false"/>
				<feature name="Salami"/>
				<feature name="Ham"/>
				<feature name="Mozzarella"/>
			</or>
			<alt mandatory="true" name="Size">
				<graphics key="collapsed" value="false"/>
				<feature name="Normal"/>
				<feature name="Big"/>
			</alt>
			<alt mandatory="true" name="Dough">
				<graphics key="collapsed" value="false"/>
				<feature name="Neapolitan"/>
				<feature name="Sicilian"/>
			</alt>
			<feature name="CheesyCrust"/>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<conj>
					<var>CheesyCrust</var>
					<var>Sicilian</var>
				</conj>
				<var>Big</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Neapolitan</var>
				<conj>
					<not>
						<var>Salami</var>
					</not>
					<not>
						<var>Ham</var>
					</not>
				</conj>
			</imp>
		</rule>
	</constraints>
</featureModel>
